{
    "name": "aedart/util",
    "description": "This package contains a set of various utility resources, which can be used independently.",
    "keywords": [
        "util",
        "array"
    ],
    "homepage": "https://github.com/aedart/util",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "aedart/license": "1.*",
        "aedart/license-file-manager": "~2.0",
        "aedart/testing": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Util\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/testCases"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1.x-dev"
        }
    }
}