{
    "name": "aedart/util",
    "description": "This package contains a set of various utility resources, which can be used independently.",
    "keywords": [
        "util",
        "array"
    ],
    "homepage": "https://github.com/aedart/util",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.2.*"
    },
    "require-dev": {
        "aedart/license": "1.*",
        "aedart/license-file-manager": "~1.7",
        "codeception/codeception": "~2.1",
        "fzaninotto/faker": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Util\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/unit/UnitTestCase.php",
            "tests/unit/collection"
        ]
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
    }
}